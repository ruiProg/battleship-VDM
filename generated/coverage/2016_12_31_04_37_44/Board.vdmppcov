class Board
types
	public CellContent = <Empty> | <Carrier> | <Battleship> | <Cruiser> | <Submarine> | <Destroyer> | <Hit> | <Miss>

instance variables
	private cells: seq of seq of CellContent := [];
	
	inv len cells = 10;
	inv card {i| i in set inds cells & len cells(i) = 10} = 10;
	
operations
	public Board: () ==> Board
		Board() == (
			for i = 1 to 10 do (
				cells(i) := [<Empty>,<Carrier>];
			);
		);
		
end Board